<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE pipeline-registry-configuration
        PUBLIC "-//Art Technology Group, Inc.//DTD PipelineRegistry//EN"
        'http://www.atg.com/dtds/pipelineregistry/pipelineregistry_1.0.dtd'>

<pipeline-registry-configuration>
  <pipeline-manager-registry xml-combine="append-without-matching">
    <pipeline-manager>
      <nucleus-path>/com/castorama/pipeline/CastoramaPipelineManager</nucleus-path>
    </pipeline-manager>
  </pipeline-manager-registry>
  <processor-registry>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/UpdateInventory</nucleus-path>
      <display-name>UpdateInventory</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/AffectAmount</nucleus-path>
      <display-name>AffectAmount</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/OrderBackup</nucleus-path>
      <display-name>OrderBackup</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/SaveBillingAddress</nucleus-path>
      <display-name>SaveBillingAddress</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/ChangeOrderStatus</nucleus-path>
      <display-name>ChangeOrderStatus</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/SendCastoOrderMessage</nucleus-path>
      <display-name>SendCastoOrderMessage</display-name>
      <category>Order</category>
    </processor>
    <processor>
      <nucleus-path>/com/castorama/commerce/order/processor/SaveInfoForOrderSummary</nucleus-path>
      <display-name>SaveInfoForOrderSummary</display-name>
      <category>Order</category>
    </processor>
  </processor-registry>
</pipeline-registry-configuration>