    <property name="phoneNumber2" column-name="PHONE_NUMBER_2" data-type="string" display-name="Téléphone 2" category="Contact"/>        
    <property name="locality" column-name="LOCALITY" data-type="string" display-name="Lieu-dit" category="Contact"/>        
