            <property name="registrationDate" display-name-resource="registrationDate" data-type="timestamp" column-name="REGISTRATION_DATE" category-resource="categoryActivity" >
                <attribute name="propertySortPriority" value="-1"/>
            </property>
            <property category-resource="categoryMiscellaneous" name="DATEMAJPROFIL"
                data-type="date" column-name="dateMAJprofil"
                display-name-resource="dateMAJprofil" >
            </property>
            <property category-resource="categoryMiscellaneous" name="typeMAJprofil"
                data-type="enumerated" default="" column-name="TYPEMAJPROFIL"
                display-name-resource="typeMAJprofil">
                <attribute name="useCodeForValue" value="true" />
                <option value="Creation" code="0" />
                <option value="Modification" code="1" />
            </property>
